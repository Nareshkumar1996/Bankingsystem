@model ApplicationUser


    <partial name="_SideBars" />
    <div class="col-md-9">
        <table class="table table-striped" border="1">
            <thead class="thead-dark">
                <tr>
                    <th>Id</th>
                    <th>Username</th>
                    <th>Email</th>
                    <th>PhoneNumber</th>
                    <th>Age</th>
                </tr>
            </thead>

            <tr>
                <td>@Model.Id</td>
                <td>@Model.UserName</td>
                <td>@Model.Email</td>
                <td>@Model.PhoneNumber</td>
                <td>@Model.Age</td>
            </tr>

        </table>

    </div>
